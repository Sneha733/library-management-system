
<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Student Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @studs.each do |table| %>
      <tr>
        <td><%= table.stud_name %></td>
        <td><%= table.stud_address %></td>
        <td>
          <%= link_to "Assign", stud_path(table) %>
        </td>
      </tr>    
    <% end %>
  </tbody>
</table>
    
<%= link_to "Create New", new_stud_path,class:"btn btn-secondary" %>
  
